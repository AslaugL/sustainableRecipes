month = {dec},
number = {10},
pages = {783--98},
pmid = {26682999},
publisher = {Mary Ann Liebert, Inc.  140 Huguenot Street, 3rd Floor New Rochelle, NY 10801 USA},
title = {{Clinically Significant Symptom Reduction in Children with Attention-Deficit/Hyperactivity Disorder Treated with Micronutrients: An Open-Label Reversal Design Study.}},
url = {http://online.liebertpub.com/doi/abs/10.1089/cap.2015.0105},
volume = {25},
