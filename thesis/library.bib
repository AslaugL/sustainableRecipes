issn = {0012-186X},
journal = {Diabetologia},
keywords = {Enteropathy,Intestinal permeability,Islet antibodies,Pre-clinical diabetes,Type 1 diabetes},
number = {12},
pages = {2824--2827},
title = {{Increased intestinal permeability precedes clinical onset of type 1 diabetes}},
