isbn = {0925-4439},
issn = {09254439},
journal = {Biochimica et Biophysica Acta - Molecular Basis of Disease},
keywords = {AdoHcy,AdoMet,Homocysteine,S-adenosyl-L-homocysteine hydrolase},
