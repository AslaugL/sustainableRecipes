issn = {1203-6765},
journal = {Pain Research and Management},
pages = {1--8},
pmid = {30453109},
publisher = {Hindawi Limited},
title = {{Physical Activity and Sleep in Chronic Fatigue Syndrome and Fibromyalgia Syndrome: Associations with Symptom Severity in the General Population Cohort LifeLines}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/30533166 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC6247578 https://www.hindawi.com/journals/prm/2018/5801510/},
