journal = {Annals of gastroenterology : quarterly publication of the Hellenic Society of Gastroenterology},
keywords = {HPA-axis,anxiety,autism,depression,enteric nervous system,gut flora,microbiome},
mendeley-tags = {HPA-axis,anxiety,autism,depression,enteric nervous system,gut flora,microbiome},
month = {jan},
