issn = {23999772},
journal = {BMJ Paediatrics Open},
keywords = {Epstein-Barr virus infection,adolescents,chronic fatigue,chronic fatigue syndrome,cognitive-behavioural therapy,music therapy},
month = {apr},
number = {1},
pages = {337--},
publisher = {BMJ Publishing Group},
title = {{Peer review history, Cognitive-behavioural Therapy Combined With Music Therapy for Chronic Fatigue Following Epstein-Barr Virus Infection in Adolescents: A Feasibility Study}},
volume = {4},
year = {2019}
}
