journal = {Scandinavian journal of gastroenterology},
keywords = {anxiety,depression,gut,gut_brain,ibs},
number = {3},
pages = {294--298},
pmid = {11916191},
title = {{Are anxiety and depression related to gastrointestinal symptoms in the general population?}},
url = {http://view.ncbi.nlm.nih.gov/pubmed/11916191},
volume = {37},
year = {2002}
}
