issn = {2045-2322},
journal = {Scientific Reports},
number = {1},
pages = {32889},
publisher = {Nature Publishing Group},
title = {{Vitamin D treatment attenuates 2,4,6-trinitrobenzene sulphonic acid (TNBS)-induced colitis but not oxazolone-induced colitis}},
