issn = {00098981},
journal = {Clinica Chimica Acta},
keywords = {Calcitriol,Dialysis,Pregnancy,Primary hyperparathyroidism,Reference values,Vitamin D},
pages = {149--153},
pmid = {26409159},
title = {{Serum calcitriol concentrations measured with a new direct automated assay in a large population of adult healthy subjects and in various clinical situations}},
