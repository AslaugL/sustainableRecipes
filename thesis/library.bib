issn = {01692046},
journal = {Landscape and Urban Planning},
keywords = {Deprivation,Green space,Residential environment,Salivary cortisol,Stress,green space,health,sosioeconomic background,stress},
mendeley-tags = {green space,health,sosioeconomic background,stress},
