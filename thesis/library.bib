title = {{The impact of microbiota on brain and behavior: Mechanisms & therapeutic potential}},
url = {http://link.springer.com/10.1007/978-1-4939-0897-4_17 http://www.ncbi.nlm.nih.gov/pubmed/24997043 http://view.ncbi.nlm.nih.gov/pubmed/24997043},
volume = {817},
