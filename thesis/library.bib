pmid = {26518233},
publisher = {BioMed Central},
title = {{Intake of macro- and micronutrients in Danish vegans}},
url = {/pmc/articles/PMC4628270/ /pmc/articles/PMC4628270/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4628270/ http://nutritionj.biomedcentral.com/articles/10.1186/s12937-015-0103-3},
