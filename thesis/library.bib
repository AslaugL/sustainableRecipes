issn = {2005-4483},
journal = {Biomolecules & Therapeutics},
keywords = {Fluorescence,Molecular dynamics simulation,Quercetin,STD-NMR,VDR},
number = {2},
pages = {191--198},
publisher = {Korean Society of Applied Pharmacology},
title = {{Quercetin Directly Interacts with Vitamin D Receptor (VDR): Structural Implication of VDR Activation by Quercetin}},
url = {http://www.biomolther.org/journal/DOIx.php?id=10.4062/biomolther.2015.122},
