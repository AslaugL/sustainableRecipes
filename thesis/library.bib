keywords = {Axon guidance,Fetal programming,Placenta,Serotonin,TPH,Tryptophan},
pages = {1--7},
pmid = {22001683},
publisher = {NIH Public Access},
title = {{Fetal, maternal, and placental sources of serotonin and new implications for developmental programming of the brain}},
