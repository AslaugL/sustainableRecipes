publisher = {Multidisciplinary Digital Publishing Institute (MDPI)},
title = {{Omega-3 Polyunsaturated Fatty Acids: Benefits and Endpoints in Sport}},
url = {/pmc/articles/PMC6357022/ /pmc/articles/PMC6357022/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6357022/ http://www.mdpi.com/2072-6643/11/1/46},
