journal = {Scientific Reports},
month = {sep},
publisher = {Nature Publishing Group},
title = {{Protein Kinase D2 Protects against Acute Colitis Induced by Dextran Sulfate Sodium in Mice}},
volume = {6},
year = {2016}
}
