journal = {Brain, Behavior, and Immunity},
keywords = {Anxiety,Depression,HPA-axis,Microbiome,Microbiota-gut-brain axis},
pmid = {28131791},
title = {{Microbes and Mental Health: A Review}},
url = {http://ac.els-cdn.com/S0889159117300168/1-s2.0-S0889159117300168-main.pdf?_tid=f8c85de4-568a-11e7-a569-00000aacb361&acdnat=1498054234_b4b85d0e19ef6c5b76d49a1e114b212e http://linkinghub.elsevier.com/retrieve/pii/S0889159117300168},
