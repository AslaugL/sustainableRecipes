journal = {Nutrition journal},
keywords = {Acetylcysteine,Acetylcysteine: administration & dosage,Antipsychotic Agents,Antipsychotic Agents: administration & dosage,Ascorbic Acid,Ascorbic Acid: administration & dosage,Databases, Factual,Diet, Gluten-Free,Dietary Supplements,Fatty Acids, Unsaturated,Fatty Acids, Unsaturated: administration & dosage,Folic Acid,Folic Acid: administration & dosage,Glutamates,Glutamates: administration & dosage,Humans,Melatonin,Melatonin: administration & dosage,NAC,Oxidative Stress,Oxidative Stress: drug effects,Randomized Controlled Trials as Topic,Schizophrenia,Schizophrenia: diet therapy,Schizophrenia: drug therapy,Thioctic Acid,Thioctic Acid: administration & dosage,Vitamin B Complex,Vitamin B Complex: administration & dosage,Vitamin D,Vitamin D: administration & dosage,Vitamin E,Vitamin E: administration & dosage,folate,nutrition,schizophrenia,trace minerals,vitamins},
