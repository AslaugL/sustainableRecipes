publisher = {NIH Public Access},
title = {{Vitamin D Status and Concomitant Autoimmunity in Celiac Disease}},
url = {http://content.wkhealth.com/linkback/openurl?sid=WKPTLP:landingpage&an=00004836-201307000-00012},
volume = {47},
