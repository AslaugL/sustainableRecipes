@incollection{Færgestad2009,
abstract = {Over the latest decades, there has been a rapid development of laboratory techniques used for analyzing the pathways from the genes to the final physiology of an organism. Insight into the biological mechanism controlling growth and development of an organism is thereby enabled at a level that was never before possible. This in turn can be utilized, for example, in health care for improved diagnostics and improved medical treatment of diseases, and in agricultural science for tailoring plants and animals with desirable properties. Both from a biological and from a data analytical point of view the scientists are facing major challenges in the research area of analyzing functional genomics data. This chapter aims at addressing some of these challenges, and discusses important questions that need to be considered as well as gives guidelines to some useful approaches for analyzing data from functional genomics. The focus of this chapter is on data modeling of empirical data from functional genomics. {\textcopyright} 2009 Elsevier B.V. All rights reserved.},
author = {F{\ae}rgestad, E. M. and Langsrud, O. and H{\o}y, M. and Hollung, K. and S{\ae}b{\o}, S. and Liland, K. H. and Kohler, A. and Gidskehaug, L. and Almergren, J. and Anderssen, E. and Martens, H.},
booktitle = {Comprehensive Chemometrics},
chapter = {4.08.2.4},
doi = {10.1016/B978-044452701-1.00011-9},
file = {:C\:/Users/aslau/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/F{\ae}rgestad et al. - 2009 - Analysis of Megavariate Data in Functional Genomics.pdf:pdf},
isbn = {9780444527011},
keywords = {Empirical data,Functional genomics,Multivariate data analysis},
month = {jan},
pages = {226},
publisher = {Elsevier},
title = {{Analysis of Megavariate Data in Functional Genomics}},
year = {2009}
}
@article{Aghapour2018,
abstract = {The epithelial lining of the airway forms the first barrier against environmental insults, such as inhaled cigarette smoke, which is the primary risk factor for the development of chronic obstructive pulmonary disease (COPD). The barrier is formed by airway epithelial junctions, which are interconnected structures that restrict permeability to inhaled pathogens and environmental stressors. Destruction of the epithelial barrier not only exposes subepithelial layers to hazardous agents in the inspired air, but also alters the normal function of epithelial cells, which may eventually contribute to the development of COPD. Of note, disruption of epithelial junctions may lead to modulation of signaling pathways involved in differentiation, repair, and proinflammatory responses. Epithelial barrier dysfunction may be particularly relevant in COPD, where repeated injury by cigarette smoke exposure, pathogens, inflammatory mediators, and impaired epithelial regeneration may compromise the barrier function. In the current review, we discuss recent advances in understanding the mechanisms of barrier dysfunction in COPD, as well as the molecular mechanisms that underlie the impaired repair response of the injured epithelium in COPD and its inability to redifferentiate into a functionally intact epithelium.},
author = {Aghapour, Mahyar and Raee, Pourya and Moghaddam, Seyed Javad and Hiemstra, Pieter S and Heijink, Irene H},
doi = {10.1165/rcmb.2017-0200TR},
issn = {1044-1549},
journal = {American Journal of Respiratory Cell and Molecular Biology},
keywords = {COPD,Cigarette smoke,Epithelial barrier function,Epithelial junctions},
number = {2},
pages = {157--169},
title = {{Airway Epithelial Barrier Dysfunction in Chronic Obstructive Pulmonary Disease: Role of Cigarette Smoke Exposure}},
url = {www.atsjournals.org http://www.atsjournals.org/doi/10.1165/rcmb.2017-0200TR},
volume = {58},
year = {2018}
}
