mendeley-tags = {awakening response,cortisol,depression,hpa-axis,trauma},
month = {mar},
pages = {24--30},
title = {{In search of the HPA axis activity in unipolar depression patients with childhood trauma: Combined cortisol awakening response and dexamethasone suppression test}},
