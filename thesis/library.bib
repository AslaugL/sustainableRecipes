issn = {20726643},
journal = {Nutrients},
keywords = {Comparison,Front-of-pack nutrition label,Funnel model,Methodological approach,Nutrient profiling},
month = {mar},
number = {3},
pages = {626},
pmid = {30875797},
publisher = {Multidisciplinary Digital Publishing Institute},
title = {{Differences and similarities between front-of-pack nutrition labels in Europe: A comparison of functional and visual aspects}},
url = {https://www.mdpi.com/2072-6643/11/3/626/htm https://www.mdpi.com/2072-6643/11/3/626},
volume = {11},
