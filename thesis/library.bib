journal = {Archives of Internal Medicine},
keywords = {bicycle ergometers,chronic fatigue syndrome,exercise,exercise stress test,exercise tolerance,fatigue,functional impairment,heart rate,muscle weakness,myalgia,respiratory quotient,surrogate endpoints,workload},
month = {nov},
number = {21},
pages = {3270--3277},
pmid = {11088089},
publisher = {American Medical Association},
title = {{Exercise capacity in chronic fatigue syndrome}},
