journal = {Clinical Nutrition},
keywords = {Afton Halloran,Dimitra Rafailia Bakaloudi,MEDLINE,Michail Chourdakis,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,PubMed Abstract,doi:10.1016/j.clnu.2020.11.035,pmid:33341313},
month = {may},
