journal = {Brain research},
keywords = {anxiety,insulin,metabolic,oxidative,rats,resistance,stress,syndrome},
month = {nov},
pages = {178--185},
pmid = {20816762},
title = {{Oxidative stress: a potential recipe for anxiety, hypertension and insulin resistance.}},
url = {http://dx.doi.org/10.1016/j.brainres.2010.08.093},
volume = {1359},
year = {2010}
