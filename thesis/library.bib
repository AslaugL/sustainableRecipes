issn = {1873-474X},
journal = {International journal of developmental neuroscience : the official journal of the International Society for Developmental Neuroscience},
keywords = {ABC,ASD,Autism Behavior Checklist,Autism spectrum disorder,BHMT,CARS,CBS,Childhood Autism Rating Scale,Chinese,Cys,GSH,GSH/GSSG,GSSG,Hcy,Homocysteine,IQR,MTHFR,N-acetylcysteine,NAC,Oxidative stress,RFC,SD,Transsulfuration metabolism,autism,autism spectrum disorders,betaine-homocysteine methyltransferase,cystathionine $\beta$-synthase,cysteine,glutathione,glutathione ratio,homocysteine,interquartile range,methylation,methylenetetrahydrofolate reductase,oxidized glutathione,reduced folate carrier,reduced glutathione,standard deviation,tGSH,total glutathione},
