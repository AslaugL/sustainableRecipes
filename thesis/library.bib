keywords = {Adherence,Diet intake,Dietary guidelines,High-income countries,Low- and middle-income countries},
month = {mar},
number = {3},
pages = {1038},
pmid = {33807053},
publisher = {Multidisciplinary Digital Publishing Institute (MDPI)},
title = {{Adherence to Food-Based Dietary Guidelines: A Systemic Review of High-Income and Low- and Middle-Income Countries}},
url = {/pmc/articles/PMC8004702/ /pmc/articles/PMC8004702/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC8004702/ https://www.mdpi.com/2072-6643/13/3/1038},
