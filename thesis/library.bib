doi = {10.1073/pnas.0706778104},
file = {:C\:/Users/aslau/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gysin et al. - 2007 - Impaired glutathione synthesis in schizophrenia convergent genetic and functional evidence.pdf:pdf},
