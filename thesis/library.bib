publisher = {Massachusetts Medical Society},
title = {{Health Effects of Overweight and Obesity in 195 Countries over 25 Years}},
url = {https://www.nejm.org/doi/full/10.1056/nejmoa1614362},
volume = {377},
year = {2017}
}
