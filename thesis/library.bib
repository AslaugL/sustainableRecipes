doi = {10.4162/nrp.2019.13.4.352},
issn = {20056168},
journal = {Nutrition Research and Practice},
keywords = {Food habits,Nutrition literacy,Obesity,Young adults,Youth},
month = {aug},
number = {4},
pages = {352--357},
pmid = {31388412},
publisher = {Korean Society of Community Nutrition and the Korean Nutrition Society},
title = {{The impact of nutrition literacy on the food habits among young adults in turkey}},
url = {/pmc/articles/PMC6669071/ /pmc/articles/PMC6669071/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6669071/},
volume = {13},
