journal = {Stress (Amsterdam, Netherlands)},
keywords = {Animals,Arginine Vasopressin,Arginine Vasopressin: genetics,Calcium-Calmodulin-Dependent Protein Kinase Type 2,DNA Methylation,Epigenesis,Genetic,Hypothalamo-Hypophyseal System,Hypothalamo-Hypophyseal System: physiology,Hypothalamo-Hypophyseal System: physiopathology,Maternal Deprivation,Methyl-CpG-Binding Protein 2,Methyl-CpG-Binding Protein 2: metabolism,Mice,Paraventricular Hypothalamic Nucleus,Paraventricular Hypothalamic Nucleus: physiology,Pituitary-Adrenal System,Pituitary-Adrenal System: physiopathology,Rats,epigenetics,hpa-axis},
