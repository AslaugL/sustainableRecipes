file = {:C\:/Users/aslau/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mehta et al. - 2011 - Using polymorphisms in FKBP5 to define biologically distinct subtypes of posttraumatic stress disorder evidence fr.pdf:pdf},
