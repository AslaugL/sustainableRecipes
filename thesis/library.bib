pmid = {33341313},
publisher = {Clin Nutr},
title = {{Intake and adequacy of the vegan diet. A systematic review of the evidence}},
url = {https://pubmed.ncbi.nlm.nih.gov/33341313/ https://linkinghub.elsevier.com/retrieve/pii/S0261561420306567},
volume = {40},
