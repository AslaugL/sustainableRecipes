pages = {92--103},
publisher = {Springer},
title = {{Energy use, blue water footprint, and greenhouse gas emissions for current food consumption patterns and dietary recommendations in the US}},
url = {https://link.springer.com/article/10.1007/s10669-015-9577-y http://link.springer.com/10.1007/s10669-015-9577-y},
