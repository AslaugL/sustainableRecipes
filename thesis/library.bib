doi = {10.1111/j.1750-3841.2010.01704.x},
issn = {00221147},
journal = {Journal of Food Science},
keywords = {Interferon-$\gamma$-inducible protein-10,Interleukin-8,Macrophage-derived chemokine,Tumor necrosis factor-$\alpha$,Vitamin D3},
number = {6},
pages = {H200--H204},
publisher = {J Food Sci},
title = {{Effects of Vitamin D3 on Expression of Tumor Necrosis Factor-$\alpha$ and Chemokines by Monocytes}},
url = {http://doi.wiley.com/10.1111/j.1750-3841.2010.01704.x},
volume = {75},
