pmid = {31146797},
publisher = {NIH Public Access},
title = {{Nutrition literacy predicts adherence to healthy/unhealthy diet patterns in adults with a nutrition-related chronic condition}},
url = {/pmc/articles/PMC6827561/ /pmc/articles/PMC6827561/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6827561/},
