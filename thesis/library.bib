journal = {Canadian journal of physiology and pharmacology},
keywords = {inflammation,metabolic,omega3,omega6,parasympathetic,syndrome},
month = {sep},
number = {9},
pages = {1171--1183},
pmid = {22913633},
title = {{Metabolic syndrome: a brain disease.}},
url = {http://dx.doi.org/10.1139/y2012-122},
volume = {90},
year = {2012}
}
