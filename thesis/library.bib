issn = {1662-5161},
journal = {Frontiers in human neuroscience},
keywords = {catecholamines,cognitive_dysfunction,stress},
pmid = {23576971},
title = {{Stress-induced cognitive dysfunction: hormone-neurotransmitter interactions in the prefrontal cortex.}},
