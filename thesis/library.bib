pages = {E3130--E3139},
pmid = {27185913},
publisher = {National Academy of Sciences},
title = {{Immunization with a heat-killed preparation of the environmental bacterium Mycobacterium vaccae promotes stress resilience in mice}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/27185913 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4896712 http://www.pnas.org/lookup/doi/10.1073/pnas.1600324113},
