publisher = {SNF Swedish Nutrition Foundation},
title = {{Differences in nutrient composition and choice of side dishes between red meat and fish dinners in Norwegian adults}},
url = {/pmc/articles/PMC4716552/ /pmc/articles/PMC4716552/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4716552/ http://foodandnutritionresearch.net/index.php/fnr/article/view/1059},
