issn = {14795876},
journal = {Journal of Translational Medicine},
keywords = {Chronic fatigue syndrome,Exercise intolerance,Exercise test,Oxygen extraction},
month = {jan},
number = {1},
pages = {20},
pmid = {24456560},
publisher = {BioMed Central},
title = {{Decreased oxygen extraction during cardiopulmonary exercise test in patients with chronic fatigue syndrome}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24456560 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3903040},
