journal = {Clinical Nutrition Experimental},
keywords = {Lactation,Microbiome,Mode of delivery,Nutrition,Perinatal},
pages = {3--16},
publisher = {Elsevier Ltd},
title = {{Perinatal nutrition: How to take care of the gut microbiota?}},
