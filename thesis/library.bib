keywords = {Adherens junctions,Airway inflammation,Asthma,Claudins,Tight junctions},
number = {1},
pages = {12--17},
publisher = {Japanese Society of Allergology},
title = {{Role of airway epithelial barrier dysfunction in pathogenesis of asthma}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S132389301730134X},
volume = {67},
year = {2018}
}
