pages = {4084},
pmid = {32521672},
publisher = {Multidisciplinary Digital Publishing Institute (MDPI)},
title = {{A Recipe for Success? A Nutrient Analysis of Recipes Promoted by Supermarkets}},
url = {/pmc/articles/PMC7312900/ /pmc/articles/PMC7312900/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC7312900/ https://www.mdpi.com/1660-4601/17/11/4084},
