isbn = {1601-5215 (Electronic)0924-2708 (Linking)},
issn = {1601-5215},
journal = {Acta neuropsychiatrica},
keywords = {angiotensin converting enzyme (ACE),association,depression,hypothalamus–pituitary–adrenal (HPA) axis,single nucleotide polymorphism (SNP)},
pages = {1--6},
pmid = {27264499},
title = {{Association analyses of depression and genes in the hypothalamus-pituitary-adrenal axis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/27264499},
year = {2016}
