journal = {Clinical Nutrition Experimental},
keywords = {CHF,CKD,Enterobacteriaceae,Gut barrier,Gut permeability,Immunity,LPS,Lactobacillus,POS,Synbiotics,TLR,chronic heart failure,chronic kidney disease,lipopolysaccharides,pectic oligosaccharides,toll-like receptor},
