pmid = {28275109},
publisher = {Wiley-Blackwell},
title = {{Intermittent and graded exercise effects on NK cell degranulation markers LAMP-1/LAMP-2 and CD8+CD38+in chronic fatigue syndrome/myalgic encephalomyelitis}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28275109 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5350160},
volume = {5},
year = {2017}
}
