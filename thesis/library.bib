@article{HelsedirektoratetDirectorateofHealth2019,
abstract = {Det har over lang tid skjedd en positiv utvikling i det norske kostholdet. Forbruket av gr{\o}nnsaker og frukt har {\o}kt betydelig over tid, og forbruket av sukker har g{\aa}tt ned de siste ti {\aa}rene. Fra midten av 1970-{\aa}rene til begynnelsen av 1990-tallet gikk kostens fett innhold ned. Kostens fettsyresammensetning endret seg i {\o}nsket retning ved at innholdet av mettede fettsyrer og transfettsyrer gikk ned. I de senere {\aa}r har imidlertid ned- gangen i kostens innhold av mettede fettsyrer stoppet opp og {\o}kt igjen. D{\o}delighe- ten av hjerteinfarkt er betydelig redusert siden 1970, mens forekomsten av kreft, fedme og type 2-diabetes fortsetter {\aa} {\o}ke. P{\aa} tross av flere positive utviklingstrekk i matvareforbruket de siste {\aa}rene har kostholdet til store deler av befolkningen fortsatt klare ern{\ae}ringsmessige svak- heter som medvirker til utvikling av hjerte- og karsykdommer, kreft, overvekt, type 2- diabetes, forstoppelse, tannr{\aa}te og jernmangel. Dersom Helsedirektoratets anbefalinger for kostholdets sammensetning og fysisk aktivitet f{\o}lges, vil fore- komsten av disse sykdommene reduseres. Det er videre en sv{\ae}rt stor utfordring {\aa} utjevne forskjeller i kosthold og andre levevaner som f{\o}rer til sosiale helseforskjeller i befolkningen. Rapporten viser utviklingen i kostholdet over tid basert p{\aa} nye tall for landets matforsyning t.o.m. 2014. Den viser ogs{\aa} nye data fra intervjuunders{\o}kelser om holdninger til kosthold og N{\o}kkelhullet. Tall fra SSBs forbruksunders{\o}kelser i privat- husholdninger 1975-2012 er publisert i den detaljerte rapporten om utviklingen i norsk kosthold.},
author = {{Helsedirektoratet (Directorate of Health)}},
