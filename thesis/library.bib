isbn = {10.3390/nu7095380},
issn = {20726643},
keywords = {FODMAP,Genetic test,Hydrogen breath test,Irritable bowel syndrome,Lactase deficiency,Lactose intolerance,Lactose malabsorption},
month = {sep},
