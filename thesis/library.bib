issn = {0143-005X},
journal = {Journal of epidemiology and community health},
keywords = {Adolescent,Adult,Aged,Child,Child, Preschool,Environment,Female,Green space,Humans,Infant,Infant, Newborn,Logistic Models,Male,Middle Aged,Netherlands,Netherlands: epidemiology,Residence Characteristics,Residence Characteristics: statistics & numerical,Rural Health,Rural Health: statistics & numerical data,Socioeconomic Factors,Urban Health,Urban Health: statistics & numerical data,health},
