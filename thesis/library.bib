issn = {1520-6394},
journal = {Depression and Anxiety},
keywords = {anxiety,epigenetics,hpa-axis,methylation,stress,therapy},
mendeley-tags = {anxiety,epigenetics,hpa-axis,methylation,stress,therapy},
month = {dec},
