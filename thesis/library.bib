pages = {590--605},
pmid = {31041447},
publisher = {Oxford University Press},
title = {{A Global Review of Food-Based Dietary Guidelines}},
url = {https://academic.oup.com/advances/article/10/4/590/5482317 /pmc/articles/PMC6628851/ /pmc/articles/PMC6628851/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6628851/},
