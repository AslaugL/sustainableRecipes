issn = {1422-0067},
journal = {International Journal of Molecular Sciences},
keywords = {3-dioxygenase,LPS,indoleamine 2,kynurenine pathway,methyltryptophan,pig,tryptophan},
mendeley-tags = {LPS,tryptophan},
month = {oct},
number = {10},
