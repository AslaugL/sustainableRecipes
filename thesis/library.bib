journal = {Frontiers in cellular neuroscience},
keywords = {Depression,Gut Microbiota,Gut–brain axis,Probiotics,depression,gut microbiota,gut-brain axis,intestinal barrier,probiotics,psychobiotics},
pages = {392},
pmid = {26528128},
publisher = {Frontiers},
title = {{Breaking down the barriers: the gut microbiome, intestinal permeability and stress-related psychiatric disorders.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26528128 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4604320 http://journal.frontiersin.org/Article/10.3389/fncel.2015.00392/abstract http://www.ncbi.nlm.nih.gov/pubmed/26528128%5Cnhttp://www.pubmedcentr},
