journal = {Biochimica et Biophysica Acta (BBA) - Molecular Basis of Disease},
keywords = {inflammation,magnesium},
number = {1},
pages = {13--21},
title = {{Low magnesium promotes endothelial cell dysfunction: implications for atherosclerosis, inflammation and thrombosis}},
url = {http://dx.doi.org/10.1016/j.bbadis.2004.01.002},
volume = {1689},
year = {2004}
}
