month = {jun},
pmid = {26092605},
title = {{Iron Deficiency Parameters in Children and Adolescents with Attention-Deficit/Hyperactivity Disorder.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26092605},
year = {2015}
}
