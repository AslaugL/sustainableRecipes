keywords = {ACTH,BDNF,C-reactive protein,CRH,CRP,CVD,Depression,HPA,HR,IL-6,KIHD,LTPA,Magnesium,N-methyl-D-aspartic acid,NMDA,PKC,PVN,Prospective study,The Kuopio Ischemic Heart Disease Risk Factor,adrenocorticotropic hormone,affective disorder,brain-derived neurotrophic factor,cardiovascular diseases,corticotropin releasing hormone,diet,hazard ratio,hypothalamic–pituitary–adrenal,interleukin 6,leisure-time physical activity,magnesium,paraventricular hypothalamic nucleus,protein kinase C},
mendeley-tags = {affective disorder,diet,magnesium},
