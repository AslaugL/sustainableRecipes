@article{Meier2015,
  title = {Environmental Impacts of Organic and Conventional Agricultural Products \textendash{} {{Are}} the Differences Captured by Life Cycle Assessment?},
  author = {Meier, Matthias S. and Stoessel, Franziska and Jungbluth, Niels and Juraske, Ronnie and Schader, Christian and Stolze, Matthias},
  year = {2015},
